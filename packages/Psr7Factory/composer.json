{
  "name": "stepthefkup/psr7-factory",
  "description": "Provides a generic way to handle pagination data from clients",
  "require": {
    "php": "^7.1",
    "symfony/psr-http-message-bridge": "^1.1",
    "zendframework/zend-diactoros": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "StepTheFkUp\\Psr7Factory\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StepTheFkUp\\Psr7Factory\\Tests\\": "tests"
    }
  }
}

{
    "name": "stepthefkup/pagination",
    "description": "Provides a generic way to handle pagination data from clients",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2",
        "illuminate/pagination": "^5.7",
        "illuminate/http": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "StepTheFkUp\\Pagination\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StepTheFkUp\\Pagination\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
